{
  "manifests": {
    "velpl/buggregator-profiler-bundle": {
      "manifest": {
        "bundles": {
          "Velpl\\BuggregatorProfilerBundle\\BuggregatorProfilerBundle": [
            "all"
          ]
        },
        "copy-from-recipe": {
          "config/": "%CONFIG_DIR%/"
        },
        "env": {
          "BUGGREGATOR_PROFILER_PORT" : "${BUGGREGATOR_PROFILER_PORT:-8000}",
          "BUGGREGATOR_PROFILER_URL" : "${BUGGREGATOR_PROFILER_URL:-http://127.0.0.1}",
          "BUGGREGATOR_BUNDLE_PROFILER_URL" : "${BUGGREGATOR_PROFILER_URL}:${BUGGREGATOR_PROFILER_PORT}",
          "BUGGREGATOR_BUNDLE_CONTAINER_NAME" : ""
        },
        "docker-compose": {
          "compose.yaml": {
            "services": {
              "buggregator-profiler": {
                "image": " ghcr.io/buggregator/server:latest",
                "container_name": " ${BUGGREGATOR_BUNDLE_CONTAINER_NAME:-buggregator-profiler}",
                "ports": [
                  "- \"${BUGGREGATOR_PROFILER_PORT}:8000\"",
                  "- \"1025:1025\"",
                  "- \"9912:9912\"",
                  "- \"9913:9913\""
                ],
                "restart": " unless-stopped"
              }
            }
          }
        }
      },
      "files": {
        "config/packages/buggregator_profiler.yaml": {
          "contents": [
            "buggregator_profiler:",
            "    enabled: false",
            "    application_name: 'Symfony App'",
            "    profiler_url: '%env(string:BUGGREGATOR_BUNDLE_PROFILER_URL)%'",
            "",
            "when@dev:",
            "    buggregator_profiler:",
            "        enabled: true",
            "        profiler_url: '%env(string:BUGGREGATOR_BUNDLE_PROFILER_URL)%'"
          ],
          "executable": false
        }
      },
      "ref": "554782be7c0bdb8378471c664d3ca8415385949b"
    }
  }
}
